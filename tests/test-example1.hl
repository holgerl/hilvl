@ var foo = 42
@ var bar = (2 + 40)
	
@ . foo == (@ . bar) then
	@ set foo = 0
	
@ var myArray = 
	1
	2
	3
	
@ . myArray loop
	@ set foo = (@ . foo + (@ . element))

@ var MyService := 
	@ var myAction :
		@ . argument + 10
		
MyService myAction (@ . foo) // foo is now 6, and this returns 16

//result: 16