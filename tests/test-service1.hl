@ new OuterService := 
	@ new outerAction :
		@ new arg1 = (@.argument)
		@.OuterService
	@ new innerAction :
		@ new arg2 = (@.argument)
		@.OuterService
	@ new innerInnerAction :
		@.arg1 + (@.arg2) + (@.argument)
		
OuterService outerAction 42 innerAction 50 innerInnerAction 100
	
