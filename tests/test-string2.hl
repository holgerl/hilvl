@ new s1 = "foo.bar"
	
@ new s2 = "first"
@ set s2 = (@.s2 + ".second")

@ new s3 = "foo : bar"
@ new s4 = "foo ( bar )"
@ new s5 = "foo \" bar"
@ new s6 = " TAB "
@ new s7 = " EOF "
@ new : = "colon"
@ new @ = "at"
@ new = = "equal"
@ new . = "dot"
@ new + = "plus"
@ new TAB = "tab"
@ new EOF = "eof"
	
@ new foo = 
	@.s1
	@.s1 + "." + "baz"
	@.s2
	@.s3
	@.s4
	@.s5
	"%%s%% hello"
	@.s6
	@.s7
	@.:
	@.@
	@.=
	@..
	@.+
	@.TAB
	@.EOF

