@ var Map := // TODO: This exports Map as a connected service, since it is on root scope
	@ var of :
		@ var MapInstance :=
			@ var tuples = (@.argument)
			@ var get :
				@ var result = _
				@ var key = (@.argument)
				@.tuples loop
					@.element get 0 == (@.key) then
						@ set result = (@.element get 1)
				@.result
			@ var with :
				@ set tuples = (@.argument)
			@ var put :
				@.tuples push (@.argument)
		@.MapInstance