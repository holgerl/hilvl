@ connect "localhost:8080" // TODO: Make it possible to use // inside strings

@ new TodoFront := 
	@ new getList :
		@ new list = ""
		Todo all _ loop // Rename Todo to TodoBackend
			IO print (@.element)
			@ new item = ("<li>" + (@.element) + "</li>")
			@ set list = (@.list + (@.item))
		@.list
	@ new onEvent :
		Todo add (@.argument)
		