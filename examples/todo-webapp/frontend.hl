@ connect "localhost:8080" // TODO: Make it possible to use // inside strings

@ var TodoFront := 
	@ var getList :
		@ var list = ""
		Todo all _ loop // Rename Todo to TodoBackend
			IO print (@.element)
			@ var item = ("<li>" + (@.element) + "</li>")
			@ set list = (@.list + (@.item))
		@.list
	@ var onEvent :
		Todo add (@.argument)
		